var mtApp = angular.module('mtApp', ['ngRoute'])
.config(function($routeProvider){
    $routeProvider
    .when('/contact', {
      templateUrl: "<%= asset_path('/assets/contact.html') %>"
    })
    .when('/resume', {
      templateUrl: 'MikeTalleyResume.pdf'
    })
    .otherwise({
      templateUrl: "<%= asset_path('/assets/mainIndex.html') %>"
    });
});
