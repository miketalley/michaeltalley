var mtApp = angular.module('mtApp', ['ngRoute', 'templates'])
.config(function($routeProvider){
    $routeProvider
    .when('/contact', {
      templateUrl: "<%=asset_path 'contact.html'%>"
    })
    .when('/resume', {
      templateUrl: 'MikeTalleyResume.pdf'
    })
    .otherwise({
      templateUrl: "<%=asset_path 'mainIndex.html' %>"
    });
});


